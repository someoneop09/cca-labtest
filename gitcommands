git

echo "# CCA" >> README.md
git init
git add README.md
git commit -m "first commit"
git branch -M main
git remote add origin https://github.com/CCA.git
git push -u origin main

git clone https://github.com/CCA.git

git fetch --all

git branch a1

git branch 

git log

git remote -v

git branch -M a1

git checkout -b a2

git pull origin main

git branch -d a1

git reset --soft
